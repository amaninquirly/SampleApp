<%= form_for @supplier do |f| %>
	<% if @supplier.errors.any? %>
	<ul class="Signup_Errors">
		<% for mesg in @supplier.errors.full_messages %>
		<li> *<%= mesg %> </li>
		<% end %>
	</ul>
	<% end %>
	<div class="supplier-form">
	<div style=" width: 12%;margin-left: 44%;">
			<%=f.text_field :name ,class:'text-field',placeholder: 'supplier name' %></br>
		</div>
		<div class="btn-submit">
			<%= f.submit :Save %>
		</div>
	</div>
<% end %>