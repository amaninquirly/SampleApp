<div style="text-align: center;border-bottom: 2px solid;padding-top: 10px;padding-bottom:9px;margin-bottom: 10px;"> listing all artcile 
</div>
<div >
<%= link_to 'New article' ,new_article_path %>
<table>
	<tr>
		<th>Title</th>
		<th>Text</th>
	</tr>
	<% @articles.each do |article| %>
	<tr>
		<td><%= article.title %></td>
		<td><%= article.text %></td>
		<td><%= link_to 'Show', article_path(article) %></td>
		<td><%= link_to 'Edit', edit_article_path(article) %></td>
		<td><%= link_to 'Destroy', article_path(article),
			method: :delete,
			data: { confirm: 'Are you sure?' } %>
		</td>
	</tr>
	<% end %>
</table>
</div>